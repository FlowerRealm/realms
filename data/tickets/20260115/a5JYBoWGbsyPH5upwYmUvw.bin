<script type="text/javascript">
var codeBlocks = document.querySelectorAll('pre');
codeBlocks.forEach(function(codeBlock) {
  var copyButton = document.createElement('buttom');
  copyButton.className = 'copy';
  var last = document.createElement('span');
  last.className = 'glyphicon glyphicon-copy';
  var cur = document.createElement('span');
  cur.className = 'glyphicon glyphicon-ok';
  copyButton.appendChild(last);
  var container = document.createElement('div');
  container.className = 'code-container';
  container.appendChild(copyButton);
  codeBlock.parentNode.insertBefore(container, codeBlock);
  container.style.position = 'relative'; 
  copyButton.addEventListener('click', function() {
    var code = codeBlock.textContent;
    var textarea = document.createElement('textarea');
    textarea.value = code;
    document.body.appendChild(textarea);
    textarea.select();
    document.execCommand('copy');
    document.body.removeChild(textarea);
    this.removeChild(last);
    this.appendChild(cur);
    this.className = 'copy_new';
    setTimeout(()=>this.className = 'copy',1000);
    setTimeout(()=>this.removeChild(cur),1000);
    setTimeout(()=>this.appendChild(last),1000);
//    setTimeout(()=>this.className = 'copy',1000);
  });
});
</script>
